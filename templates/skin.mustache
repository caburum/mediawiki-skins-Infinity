<main class="mw-body">
	<div id="siteNotice">{{{html-site-notice}}}</div>

	{{{html-user-message}}}

	<div class="mw-indicators mw-body-content">
	{{#array-indicators}}
		<div id="{{id}}" class="{{class}}">{{{html}}}</div>
	{{/array-indicators}}
	</div>

	<header class="mw-body-header">
		<div class="mw-body-header-top">
			{{#data-portlets.data-languages}}{{>Portlet}}{{/data-portlets.data-languages}}
		</div>
		<div class="mw-body-header-bottom">
			<div class="mw-body-header-title">
				<h1 class="firstHeading" id="firstHeading" {{{html-user-language-attributes}}}>{{{html-title}}}</h1>
				<div id="siteSub">{{msg-tagline}}</div>
			</div>
			{{#data-pagetools}}
			<div class="mw-body-header-actions">
				{{#data-page-views}}
					<nav id="p-views">{{{html-items}}}</nav>
				{{/data-page-views}}
				<div class="mw-dropdown">
					<div class="mw-dropdown-toggle">
						toggle
					</div>
					<div class="mw-dropdown-content">
						{{#data-page-actions}}{{>Menu}}{{/data-page-actions}}
						{{#data-namespaces}}{{>Menu}}{{/data-namespaces}}
						{{#data-variants}}{{>Menu}}{{/data-variants}}
						{{#data-page-toolbox}}{{>Menu}}{{/data-page-toolbox}}
					</div>
				</div>
			</div>
			{{/data-pagetools}}
		</div>
	</header>

	<div class="mw-body-content" id="content">
		<div id="contentSub">
			{{{html-subtitle}}}
			{{{html-undelete-link}}}
		</div>
		{{{html-body-content}}}

		<!-- todo: replace with .mixin-clearfix -->
		<div class="visualClear"></div>

		{{{html-categories}}}
	</div>

	{{{html-after-content}}}
</main>
<div id="footer" class="mw-footer" role="contentinfo" {{{html-user-language-attributes}}}>
	{{#data-footer}}
	{{!}}{{#data-icons}}{{>FooterList}}{{/data-icons}}
	{{!}}<div id="footer-list">
	{{!}}{{#data-info}}{{>FooterList}}{{/data-info}}
	{{!}}{{#data-places}}{{>FooterList}}{{/data-places}}
	{{!}}</div>
	{{/data-footer}}
	<!-- todo: replace with .mixin-clearfix -->
	<div class="visualClear"></div>
</div>